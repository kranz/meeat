<h1>Listing closing_days</h1>

<table>
  <tr>
    <th>Time table</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @closing_days.each do |closing_day| %>
  <tr>
    <td><%= closing_day.time_table_id %></td>
    <td><%= closing_day.start_date %></td>
    <td><%= closing_day.end_date %></td>
    <td><%= link_to 'Show', closing_day %></td>
    <td><%= link_to 'Edit', edit_closing_day_path(closing_day) %></td>
    <td><%= link_to 'Destroy', closing_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Closing day', new_closing_day_path %>
