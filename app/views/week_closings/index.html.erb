<h1>Listing week_closings</h1>

<table>
  <tr>
    <th>Time table</th>
    <th>Weekday</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @week_closings.each do |week_closing| %>
  <tr>
    <td><%= week_closing.time_table_id %></td>
    <td><%= week_closing.weekday %></td>
    <td><%= week_closing.type %></td>
    <td><%= link_to 'Show', week_closing %></td>
    <td><%= link_to 'Edit', edit_week_closing_path(week_closing) %></td>
    <td><%= link_to 'Destroy', week_closing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Week closing', new_week_closing_path %>
